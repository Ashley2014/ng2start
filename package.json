{
  "name": "ng2start",
  "version": "1.0.0",
  "description": "ng2start",
  "scripts": {
    "dev": "npm run server:dev:hmr",
    "build": "npm run build:prod",
    "server:prod": "http-server dist -c-1 --cors",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "start": "npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashley2014/ng2start.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ashley2014/ng2start/issues"
  },
  "homepage": "https://github.com/Ashley2014/ng2start#readme",
  "dependencies": {
    "@angular/animations": "^4.3.2",
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.3.2",
    "@angular/core": "^4.3.2",
    "@angular/forms": "^4.3.2",
    "@angular/http": "^4.3.2",
    "@angular/platform-browser": "^4.3.2",
    "@angular/platform-browser-dynamic": "^4.3.2",
    "@angular/router": "^4.3.2",
    "@angularclass/hmr": "^2.1.3",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.3.2",
    "@angularclass/hmr-loader": "^3.0.4",
    "@types/hammerjs": "^2.0.34",
    "@types/node": "^8.0.17",
    "add-asset-html-webpack-plugin": "^2.0.1",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.2.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "http-server": "^0.10.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^3.1.0",
    "node-sass": "^4.5.3",
    "optimize-js-plugin": "0.0.4",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  }
}
